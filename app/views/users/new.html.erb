<h1 class="reg-title"> Enter the JUNGLE!</h1>
<h6 class="reg-title"> Sign up using the below form:</h6>
<%= form_for @user do |f| %>

  <table>
    <tr class="reg-row">
      <td class="reg-field-head">
        First Name:
      </td>
      <td>
        <%= f.text_field :first_name %>
      </td>
    </tr>
    <tr class="reg-row">
      <td class="reg-field-head">
        Last Name:
      </td>
      <td> 
        <%= f.text_field :last_name %>
      </td>
    </tr>
    <tr class="reg-row">
      <td class="reg-field-head">
        Email:
      </td>
      <td>
        <%= f.text_field :email %>
      </td>
    </tr>
    <tr class="reg-row">
      <td class="reg-field-head">
        Password:
      </td>
      <td>
        <%= f.password_field :password %>
      </td>
    </tr>
    <tr class="reg-row">
      <td class="reg-field-head">
        Password Confirmation:
      </td>
      <td>
        <%= f.password_field :password_confirmation %>
      </td>
    </tr>
    <tr class="reg-row">
      <td></td>
      <td class="reg-submit">
        <%= f.submit "Submit" %>
      </td>
    </tr>

<% end %>